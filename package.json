{
  "name": "notification-server",
  "packageManager": "yarn@4.0.0",
  "version": "1.0.0",
  "description": "",
  "license": "Apache-2.0",
  "author": "AnzuSystems",
  "contributors": [
    {
      "name": "AnzuSystems Community",
      "homepage": "https://github.com/orgs/anzusystems/people"
    }
  ],
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "yarn clean && yarn lint --fix && tsc",
    "production": "yarn build",
    "prod": "NODE_OPTIONS=\"--max-old-space-size=200\" node dist/index.js --max-http-header-size=16000",
    "dev": "TS_NODE=true NODE_OPTIONS=\"-r ts-node/register\" node src/index.ts --max-http-header-size=16000",
    "watch": "nodemon --watch src -e ts --exec yarn dev",
    "test": "jest --coverage=true --runInBand"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^3.1.0",
    "cookies": "^0.8.0",
    "dotenv": "^16.0.1",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.10.0",
    "typescript": "^4.7.4",
    "winston": "^3.8.1",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.7",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.6.3",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0"
  }
}
