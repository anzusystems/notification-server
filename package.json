{
  "name": "notification-server",
  "packageManager": "yarn@4.0.2",
  "version": "1.1.0",
  "description": "",
  "license": "Apache-2.0",
  "author": "AnzuSystems",
  "contributors": [
    {
      "name": "AnzuSystems Community",
      "homepage": "https://github.com/orgs/anzusystems/people"
    }
  ],
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.ts ; ",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn lint --fix && tsc",
    "production": "yarn build",
    "prod": "NODE_OPTIONS=\"--max-old-space-size=200\" node dist/index.js --max-http-header-size=16000",
    "dev": "TS_NODE=true NODE_OPTIONS=\"-r ts-node/register\" node src/index.ts --max-http-header-size=16000",
    "watch": "nodemon --watch src -e ts --exec yarn dev",
    "tsc": "tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.0.7",
    "cookies": "^0.8.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.1.14",
    "typedi": "^0.10.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "ws": "^8.15.1"
  },
  "devDependencies": {
    "@types/cookies": "^0.7.10",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tslib": "^2.6.2"
  }
}
