{
  "name": "notification-server",
  "packageManager": "yarn@4.2.2",
  "version": "1.1.0",
  "description": "",
  "license": "Apache-2.0",
  "author": "AnzuSystems",
  "contributors": [
    {
      "name": "AnzuSystems Community",
      "homepage": "https://github.com/orgs/anzusystems/people"
    }
  ],
  "main": "dist/index.js",
  "scripts": {
    "ci": "run-s --print-name lint:tsc lint:eslint",
    "lint": "yarn ci",
    "lint:eslint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:eslint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:prettier": "prettier -c \"./src/**/*.{ts,js}\"",
    "lint:prettier:fix": "prettier -w \"./src/**/*.{ts,js}\"",
    "lint:tsc": "tsc --noEmit -p tsconfig.json --composite false",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn lint && tsc",
    "production": "yarn build",
    "prod": "NODE_OPTIONS=\"--max-old-space-size=200\" node dist/index.js --max-http-header-size=16000",
    "dev": "TS_NODE=true NODE_OPTIONS=\"-r ts-node/register\" node src/index.ts --max-http-header-size=16000",
    "watch": "nodemon --watch src -e ts --exec yarn dev",
    "test": "jest --coverage=true --runInBand"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.1.1",
    "cookies": "^0.9.1",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "typedi": "^0.10.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.5.3",
    "@types/cookies": "^0.7.10",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^50.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2"
  }
}
